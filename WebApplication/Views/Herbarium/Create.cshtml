@model Herbarium

@{
  ViewBag.Title = "New Herbarium";
}

<h2>Enter the page you want</h2>

<form asp-action="Create" method="post">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label asp-for="InventoryNumber"></label>
        <div><span asp-validation-for="InventoryNumber" class="text-danger" /></div>
        <input asp-for="InventoryNumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="YearOfCollection"></label>
        <div><span asp-validation-for="YearOfCollection" class="text-danger" /></div>
        <input asp-for="YearOfCollection" class="form-control" />
    </div>
    <div class="text-center">
        <button class="btn btn-primary" type="submit">Add</button>
        <a asp-action="Index" class="btn btn-secondary">Back</a>
    </div>
</form>

@section scripts {
  <partial name="IncludeValidation" />
}